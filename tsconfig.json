{
  "compilerOptions": {
    "module": "umd",
    "jsx": "preserve",
    "esModuleInterop": true,
    "sourceMap": true,
    "allowJs": false,
    "lib": [
      "esnext",
      "dom"
    ],
    "target": "ESNext",
    "rootDir": "src",
    "declaration": true,
    "outDir": "dist",
    "moduleResolution": "node"
  },
  "exclude":[
    "dist",
    "node_modules",
    "**/*.spec.ts",
    "Benchmarks"
  ]
}